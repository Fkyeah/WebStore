@{
    ViewBag.Title = "Employers";
}


<div class="container">
    <h1>Сотрудники</h1>
    <table class="table">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>LastName</td>
            <td>Patronymic</td>
            <td>Age</td>
            <td>Details</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.LastName</td>
                <td>@item.Patronymic</td>
                <td>@item.Age</td>
                <td><a asp-action="Details" asp-route-id="@item.Id">Детали</a></td>
            </tr>
        }
    </table>
</div>