@model IEnumerable<BrandViewModel>

<div class="brands_products">
    <!--brands_products-->
    <h2>Brands</h2>
    <div class="brands-name">
        <ul class="nav nav-pills nav-stacked">
            @foreach (var el in Model)
            {
                <li><a asp-controller="Shop" asp-action="Index" asp-route-BrandId="@el.Id"> <span class="pull-right">(@el.Order)</span>@el.Name</a></li>
            }
        </ul>
    </div>
</div><!--/brands_products-->